\n          value: 3,\r\n          label: 3\r\n        }, {\r\n          value: 4,\r\n          label: 4\r\n        }, {\r\n          value: 5,\r\n          label: 5\r\n        }, {\r\n          value: 6,\r\n          label: 6\r\n        }, {\r\n          value: 7,\r\n          label: 7\r\n        }, {\r\n          value: 8,\r\n          label: 8\r\n        }, {\r\n          value: 9,\r\n          label: 9\r\n        }, {\r\n          value: 10,\r\n          label: 10\r\n        }, {\r\n          value: 11,\r\n          label: 11\r\n        }, {\r\n          value: 12,\r\n          label: 12\r\n        }, {\r\n          value: 13,\r\n          label: 13\r\n        }, {\r\n          value: 14,\r\n          label: 14\r\n        }, {\r\n          value: 15,\r\n          label: 15\r\n        }, {\r\n          value: 16,\r\n          label: 16\r\n        }],\r\n        pickerOptions: {\r\n          disabledDate(time) {\r\n            return time.getTime() < Date.now() - 3600 * 1000 * 24;\r\n          }\r\n        },\r\n      }\r\n    },\r\n    methods: {\r\n      DateChange(date) {\r\n        this.doform.date = formatDate(date, 'yyyy-MM-dd')\r\n      },\r\n      TimeChange(time) {\r\n        this.doform.time = formatDate(time, 'hh:mm')\r\n      },\r\n      hanleSubmit() {\r\n        const res = this.doform\r\n        console.log(res);\r\n        if (res.from == \"\") {\r\n          this.show(\"Please Enter Departure\")\r\n        }\r\n        else if (res.to == \"\") {\r\n          this.show(\"Please Select Airport\")\r\n        }\r\n        else if (res.date == \"\") {\r\n          this.show(\"Please Select Date\")\r\n        }\r\n        else if (res.phone == \"\") {\r\n          this.show(\"Please Enter Phone\")\r\n        }\r\n        else if (res.time == \"\") {\r\n          this.show(\"Please Select Time\")\r\n        }\r\n        else if (res.people == \"\") {\r\n          this.show(\"Please Select Num Of People\")\r\n        }\r\n        else if (res.luggages == 